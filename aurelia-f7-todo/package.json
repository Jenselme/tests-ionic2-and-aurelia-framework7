{
  "name": "aurelia-skeleton-navigation-webpack",
  "version": "0.1.0",
  "description": "A starter kit for building a standard navigation-style app with Aurelia and webpack.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --hot --inline --progress --devtool eval",
    "build": "webpack --config webpack.config.js --progress --profile",
    "build-android": "webpack -p --config webpack.prod.config.js --progress && cordova build android",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aurelia/skeleton-navigation.git"
  },
  "keywords": [
    "aurelia",
    "skeleton",
    "navigation",
    "webpack"
  ],
  "author": "Martijn Boland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aurelia/skeleton-navigation/issues"
  },
  "homepage": "https://github.com/aurelia/skeleton-navigation#readme",
  "aurelia": {
    "build": {
      "moduleRootOverride": {
       "aurelia-templating-resources": "dist/commonjs"
      }
    }
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "1.0.0-beta.2.0.2",
    "aurelia-event-aggregator": "1.0.0-beta.2.0.1",
    "aurelia-fetch-client": "1.0.0-beta.2.0.1",
    "aurelia-framework": "1.0.0-beta.2.0.1",
    "aurelia-history-browser": "1.0.0-beta.2.0.1",
    "aurelia-logging-console": "1.0.0-beta.2.0.1",
    "aurelia-pal-browser": "1.0.0-beta.2.0.1",
    "aurelia-polyfills": "1.0.0-beta.2.0.1",
    "aurelia-router": "1.0.0-beta.2.0.1",
    "aurelia-templating-binding": "1.0.0-beta.2.0.2",
    "aurelia-templating-resources": "1.0.0-beta.3.0.4",
    "aurelia-templating-router": "1.0.0-beta.2.0.3",
    "aurelia-validation": "^0.9.2",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.6.3",
    "framework7": "^1.4.2",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.4.2"
  },
  "devDependencies": {
    "aurelia-tools": "0.2.2",
    "aurelia-webpack-plugin": "1.0.0-beta.1.0.4",
    "babel-core": "^5.8.35",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^5.4.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.21.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-webpack": "^1.7.0",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
