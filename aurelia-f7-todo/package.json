    "babel-preset-es2015-loose": "8.0.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.18.0",
    "babel-loader": "6.2.10",
    "css-loader": "0.26.1",
    "eslint": "3.14.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.26.0",
    "jasmine-core": "2.5.2",
    "karma": "1.4.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "^1.1.0",
