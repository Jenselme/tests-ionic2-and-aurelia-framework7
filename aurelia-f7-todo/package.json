{
  "name": "aurelia-f7-todo",
  "version": "0.1.0",
  "description": "A starter kit for building a standard navigation-style app with Aurelia and webpack.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --inline --progress --devtool eval",
    "build": "webpack --config webpack.config.js --progress --profile",
    "build-android": "webpack -p --config webpack.prod.config.js --progress && cordova build android",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aurelia/skeleton-navigation.git"
  },
  "keywords": [
    "aurelia",
    "skeleton",
    "navigation",
    "webpack"
  ],
  "author": "Julien Enselme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aurelia/skeleton-navigation/issues"
  },
  "homepage": "https://github.com/aurelia/skeleton-navigation#readme",
  "aurelia": {
    "build": {
      "moduleRootOverride": {
       "aurelia-templating-resources": "dist/commonjs"
      }
    }
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "1.1.0",
    "aurelia-event-aggregator": "1.0.1",
    "aurelia-fetch-client": "1.1.0",
    "aurelia-framework": "1.0.8",
    "aurelia-history-browser": "1.0.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-pal-browser": "1.1.0",
    "aurelia-polyfills": "1.1.1",
    "aurelia-router": "1.1.1",
    "aurelia-templating-binding": "1.2.0",
    "aurelia-templating-resources": "1.2.0",
    "aurelia-templating-router": "1.0.1",
    "aurelia-validation": "1.0.0-beta.1.0.1",
    "bootstrap": "3.3.7",
    "font-awesome": "4.7.0",
    "framework7": "1.5.2",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "1.4.3"
  },
  "devDependencies": {
    "aurelia-tools": "1.0.0",
    "aurelia-webpack-plugin": "1.2.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
